            RowBox[{"0", ",", "2"}], "}"}]}], "}"}]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<m = \>\"", ",", " ", 
