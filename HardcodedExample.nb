Cell[BoxData[
 RowBox[{
  RowBox[{"HardcodedExample", "[", 
   RowBox[{"{", 
    RowBox[{"min_", ",", "max_"}], "}"}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"x", " ", "=", " ", "min"}], ",", 
      RowBox[{"y", "=", "max"}], ",", " ", "a", ",", "b", ",", "Smatrix", ",",
       "Simatrix", ",", "M", " ", ",", "s", ",", "j", ",", " ", "V", ",", 
      "m"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"m", " ", "=", " ", 
      RowBox[{"LinearAlgebra`BlockDiagonalMatrix", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"1", "+", "\[ImaginaryI]"}], ",", "1"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"1", "+", "\[ImaginaryI]"}]}], "}"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"2", ",", "1", ",", "0"}], "}"}], ",", 
