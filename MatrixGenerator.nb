       "w", ",", "min", ",", "max", ",", "listSup", ",", "listDiag", ",", 
       "sub", ",", "d", ",", "diag", ",", "dim"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"min", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"max", "=", "1"}], ";", "\[IndentingNewLine]", 
      RowBox[{"dim", "=", 
       RowBox[{"Total", "[", "j", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"v", "=", 
       RowBox[{"RandomInteger", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"min", ",", "max"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"dim", ",", "dim"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
