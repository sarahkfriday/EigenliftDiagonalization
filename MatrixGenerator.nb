           "diagonal", " ", "where", " ", "n", " ", "is", " ", "size", " ", 
           "of", " ", "jordan", " ", "block"}]}], " ", "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"listSup", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"listSup", ",", 
            RowBox[{"Table", "[", 
             RowBox[{"1", ",", 
              RowBox[{
               RowBox[{"j", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "-", "1"}]}], "]"}]}], 
           "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"(*", " ", 
          RowBox[{"add", " ", 
           RowBox[{"0", "'"}], "s", " ", "to", " ", "separate", " ", "jordan",
            " ", "blocks", " ", "on", " ", "super", " ", "diagonal"}], " ", 
          "*)"}], "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"i", "\[NotEqual]", 
            RowBox[{"Length", "[", "j", "]"}]}], ",", 
           RowBox[{"listSup", "=", 
            RowBox[{"Append", "[", 
             RowBox[{"listSup", ",", "0"}], "]"}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"(*", " ", 
          RowBox[{"add", " ", "j", 
           RowBox[{"(", "i", ")"}], " ", "instances", " ", "of", " ", "the", 
           " ", "eigenvalue", " ", "to", " ", "the", " ", "diagonal"}], " ", "*)"}]\
, "\[IndentingNewLine]", 
         RowBox[{"listDiag", "=", 
          RowBox[{"Append", "[", 
           RowBox[{"listDiag", ",", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"ev", "[", 
               RowBox[{"[", "i", "]"}], "]"}], ",", 
              RowBox[{"j", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], "]"}]}], "]"}]}], ";"}]}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}], ";", 
      RowBox[{"(*", " ", 
       RowBox[{"End", " ", "For", " ", "loop"}], " ", "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"sup", "=", 
       RowBox[{"DiagonalMatrix", "[", 
        RowBox[{
         RowBox[{"Flatten", "[", "listSup", "]"}], ",", "1"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"d", "=", 
